{
  "expo": {
    "name": "VitaCheck",
    "slug": "vitacheck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vitalogo-final.png",
    "scheme": "vitacheck",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["background-processing", "background-fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vitalogo-icon.png",
        "backgroundColor": "#030712"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.georgiam.vitacheck",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "WAKE_LOCK",
        "VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vitalogo-final.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#030712"
        }      
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/vitalogo-final.png",
          "color": "#36d399",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1b8dba3f-cbd1-4cdb-b954-fb182bc78fee"
      }
    }
  }
}